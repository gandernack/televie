      'field_section_emotionnel_2' => array(
        'und' => array(
          array(
            'value' => "<div id=\"slide2\">\r\n\r\n  <div class=\"section-emo\" id=\"section-emo-2\">\r\n      <div class=\"wrapper\">\r\n          <div class=\"content\">\r\n              <h3>La recherche,</h3>\r\n\r\n              <h2>seule solution contre le cancer</h2>\r\n\r\n              <p class=\"desc\">Si la vie n’a pas de prix… la recherche a un coût. Chaque euro récolté pendant l’opération Télévie est intégralement versé à la lutte contre le cancer.<br />\r\n                  <br /> Chaque homme, chaque femme, chaque enfant a le droit d’être en bonne santé. Tout le monde est touché de près ou de loin par le cancer. Pour combattre cet ennemi la recherche est notre seule arme !</p>\r\n          </div>\r\n      </div>\r\n\r\n      <div class=\"section-emo-2-par\">\r\n          <div class=\"wrapper\">\r\n              <p>Arsène Burny</p>\r\n\r\n              <p><strong>« La lutte contre les cancers est intense et basée sur la connaissance. Il faut des cerveaux, des bras et de l’argent ! »</strong></p>\r\n          </div>\r\n      </div>\r\n  </div>\r\n\r\n</div>\r\n",
            'format' => NULL,
            'safe_value' => "&lt;div id=&quot;slide2&quot;&gt;\r\n\r\n  &lt;div class=&quot;section-emo&quot; id=&quot;section-emo-2&quot;&gt;\r\n      &lt;div class=&quot;wrapper&quot;&gt;\r\n          &lt;div class=&quot;content&quot;&gt;\r\n              &lt;h3&gt;La recherche,&lt;/h3&gt;\r\n\r\n              &lt;h2&gt;seule solution contre le cancer&lt;/h2&gt;\r\n\r\n              &lt;p class=&quot;desc&quot;&gt;Si la vie n’a pas de prix… la recherche a un coût. Chaque euro récolté pendant l’opération Télévie est intégralement versé à la lutte contre le cancer.&lt;br /&gt;\r\n                  &lt;br /&gt; Chaque homme, chaque femme, chaque enfant a le droit d’être en bonne santé. Tout le monde est touché de près ou de loin par le cancer. Pour combattre cet ennemi la recherche est notre seule arme !&lt;/p&gt;\r\n          &lt;/div&gt;\r\n      &lt;/div&gt;\r\n\r\n      &lt;div class=&quot;section-emo-2-par&quot;&gt;\r\n          &lt;div class=&quot;wrapper&quot;&gt;\r\n              &lt;p&gt;Arsène Burny&lt;/p&gt;\r\n\r\n              &lt;p&gt;&lt;strong&gt;« La lutte contre les cancers est intense et basée sur la connaissance. Il faut des cerveaux, des bras et de l’argent ! »&lt;/strong&gt;&lt;/p&gt;\r\n          &lt;/div&gt;\r\n      &lt;/div&gt;\r\n  &lt;/div&gt;\r\n\r\n&lt;/div&gt;\r\n",
          ),
        ),
      ),
      'field_section_emotionnel_3' => array(
        'und' => array(
          array(
            'value' => "<div id=\"slide3\">\r\n\r\n    <div class=\"section-emo\" id=\"section-emo-3\">\r\n        <div class=\"wrapper\">\r\n            <div class=\"content\">\r\n                <h2>Les résultats sont là !</h2>\r\n\r\n                <h3>Hier, 80% des enfants malades mourraient, aujourd'hui plus de 80% survivent !</h3>\r\n\r\n                <p class=\"desc\">Votre générosité fait reculer la maladie et sauve des vies ! Grâce à vos dons, 111 chercheurs travaillent au quotidien dans ce combat contre les cancers.<br />\r\n                    <br /> Leurs projets progressent, mais le cancer a encore et toujours une longueur d’avance.<br />\r\n                    <br /> Aujourd’hui, plus que jamais, le Télévie a besoin de vous !</p>\r\n            </div>\r\n        </div>\r\n\r\n        <div class=\"section-emo-3-par\">\r\n            <div class=\"wrapper\">\r\n                <h2>Grâce à vos dons</h2>\r\n\r\n                <div class=\"bullet\"><a class=\"bullet-1\" href=\"#\"><span class=\"visu-wrapper\"><span class=\"visu\">&nbsp;</span> </span> <strong>2114</strong> <span class=\"desc\">projets de recherche</span> </a>\r\n                    <a class=\"bullet-2\" href=\"http://dons.televie.be\"> <span class=\"visu-wrapper\"> <span class=\"visu\">&nbsp;</span> </span> <strong>1€ versé</strong> <span class=\"desc\">= 1€ à la recherche</span> </a>\r\n                    <a class=\"bullet-3\" href=\"#\"> <span class=\"visu-wrapper\"> <span class=\"visu\">&nbsp;</span> </span> <strong>2133</strong> <span class=\"desc\">scientifiques</span> </a>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    </div>\r\n\r\n\r\n</div>",
            'format' => NULL,
            'safe_value' => "&lt;div id=&quot;slide3&quot;&gt;\r\n\r\n    &lt;div class=&quot;section-emo&quot; id=&quot;section-emo-3&quot;&gt;\r\n        &lt;div class=&quot;wrapper&quot;&gt;\r\n            &lt;div class=&quot;content&quot;&gt;\r\n                &lt;h2&gt;Les résultats sont là !&lt;/h2&gt;\r\n\r\n                &lt;h3&gt;Hier, 80% des enfants malades mourraient, aujourd&#039;hui plus de 80% survivent !&lt;/h3&gt;\r\n\r\n                &lt;p class=&quot;desc&quot;&gt;Votre générosité fait reculer la maladie et sauve des vies ! Grâce à vos dons, 111 chercheurs travaillent au quotidien dans ce combat contre les cancers.&lt;br /&gt;\r\n                    &lt;br /&gt; Leurs projets progressent, mais le cancer a encore et toujours une longueur d’avance.&lt;br /&gt;\r\n                    &lt;br /&gt; Aujourd’hui, plus que jamais, le Télévie a besoin de vous !&lt;/p&gt;\r\n            &lt;/div&gt;\r\n        &lt;/div&gt;\r\n\r\n        &lt;div class=&quot;section-emo-3-par&quot;&gt;\r\n            &lt;div class=&quot;wrapper&quot;&gt;\r\n                &lt;h2&gt;Grâce à vos dons&lt;/h2&gt;\r\n\r\n                &lt;div class=&quot;bullet&quot;&gt;&lt;a class=&quot;bullet-1&quot; href=&quot;#&quot;&gt;&lt;span class=&quot;visu-wrapper&quot;&gt;&lt;span class=&quot;visu&quot;&gt;&amp;nbsp;&lt;/span&gt; &lt;/span&gt; &lt;strong&gt;2114&lt;/strong&gt; &lt;span class=&quot;desc&quot;&gt;projets de recherche&lt;/span&gt; &lt;/a&gt;\r\n                    &lt;a class=&quot;bullet-2&quot; href=&quot;http://dons.televie.be&quot;&gt; &lt;span class=&quot;visu-wrapper&quot;&gt; &lt;span class=&quot;visu&quot;&gt;&amp;nbsp;&lt;/span&gt; &lt;/span&gt; &lt;strong&gt;1€ versé&lt;/strong&gt; &lt;span class=&quot;desc&quot;&gt;= 1€ à la recherche&lt;/span&gt; &lt;/a&gt;\r\n                    &lt;a class=&quot;bullet-3&quot; href=&quot;#&quot;&gt; &lt;span class=&quot;visu-wrapper&quot;&gt; &lt;span class=&quot;visu&quot;&gt;&amp;nbsp;&lt;/span&gt; &lt;/span&gt; &l